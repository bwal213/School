 */
public class Test1 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		double[] ary = new double[10];
		double tmp = 0;
		
		for(int i=0;i < ary.length; i++){
			
			tmp = ary[i] + tmp;
			
		}
		
		System.out.print("The avg is: " + (tmp / ary.length));
	}

}
